@inject NavigationManager navigationManager
@inherits LayoutComponentBase


<base href="~/"

<div class="page">
    

    <main>
        <nav class="navbar-toggler bg-body-tertiary">
            <div class="container-fluid" style="height: 60px">
                <a class="navbar-brand" href="#">
                    <img src="data:image/jpeg;base64,"
                         alt=""
                         width="30"
                         height="24"
                         class="d-inline-block align-text-top" />Livro de Receitas
                </a>
              
                <NavLink class="button btn btn-outline-warning" href="CriarReceita" Match="NavLinkMatch.All">
                    <span class="bi bi-house-door-fill-nav-menu" aria-hidden="true"></span> Criar Receita
                </NavLink>

               
            </div>
        </nav>

        <article class="content px-4">
            @Body
        </article>
    </main>
</div>

<div id="blazor-error-ui">
    An unhandled error has occurred.
    <a href="" class="reload">Reload</a>
    <a class="dismiss">🗙</a>
</div>

